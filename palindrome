def is_palindrome(x: int) -> bool:
    if x < 0:  # Negative numbers are not palindromes
        return False
    return str(x) == str(x)[::-1]

# Example usage:
x = 121
print(is_palindrome(x))  # Output: True

x = -121
print(is_palindrome(x))  # Output: False

x = 10
print(is_palindrome(x))  # Output: False
