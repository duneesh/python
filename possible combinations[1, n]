def combine(n, k):
    def backtrack(start, curr):
        if len(curr) == k:
            result.append(curr[:])
            return
        for i in range(start, n + 1):
            curr.append(i)
            backtrack(i + 1, curr)
            curr.pop()

    result = []
    backtrack(1, [])
    return result

# Example usage:
n = 4
k = 2
print(combine(n, k))
